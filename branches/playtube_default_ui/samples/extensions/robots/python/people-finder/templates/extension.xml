<extension 
    name="People Finder"
    thumbnailUrl="http://{{ appid }}.appspot.com/static/icon.png"
    description="Find attendees at {{ title }} that share your interests."
    version="1"> 
  <author name="Google"/> 
  <menuHook location="NEW_WAVE_MENU" text="New People Finder Profile"
    iconUrl="https://{{ appid }}.appspot.com/static/icon.png"> 
      <createNewWave >
        <participant id='{{ appid }}@appspot.com'/>
      </createNewWave>
  </menuHook> 
  <menuHook location="toolbar" text="Add People Search Gadget"
    iconUrl="https://{{ appid }}.appspot.com/static/icon.png"> 
    <insertGadget url="http://{{ appid }}.appspot.com/gadget.xml"/> 
  </menuHook> 
</extension> 
